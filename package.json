{
  "name": "feedbacky",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "yarn --cwd ./client run start",
    "dev": "concurrently --k \"yarn run server\" \"yarn run client\" \"yarn run webhook\"",
    "heroku-postbuild": "YARN_PRODUCTION=false yarn --cwd ./client install && yarn --cwd ./client run build",
    "webhook": "ssh -tt -R 80:localhost:5000 ssh.localhost.run"
  },
  "license": "MIT",
  "dependencies": {
    "@passport-next/passport": "^2.1.1",
    "@passport-next/passport-google-oauth2": "^1.0.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^2.0.0-rc.1",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.10",
    "path-parser": "^4.2.0",
    "ramda": "^0.26.1",
    "sendgrid": "^5.2.3",
    "stripe": "^6.36.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1"
  }
}
