{
  "name": "feedbacky",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "yarn --cwd ./client run start",
    "dev": "concurrently --k \"yarn run server\" \"yarn run client\" \"yarn run webhook\"",
    "heroku-postbuild": "YARN_PRODUCTION=false yarn --cwd ./client install && yarn --cwd ./client run build",
    "webhook": "ssh -tt -R 80:localhost:5000 ssh.localhost.run"
  },
  "license": "MIT",
  "dependencies": {
    "@passport-next/passport": "^3.1.0",
    "@passport-next/passport-google-oauth2": "^1.0.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^2.0.0-rc.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.19",
    "path-parser": "^6.1.0",
    "ramda": "^0.27.1",
    "sendgrid": "^5.2.3",
    "stripe": "^8.137.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  }
}
