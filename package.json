{
  "name": "feedbacky",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "11.11.0",
    "npm": "6.7.0"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "yarn --cwd ./client run start",
    "dev": "concurrently --k \"yarn run server\" \"yarn run client\"",
    "heroku-postbuild": "YARN_PRODUCTION=false yarn --cwd ./client install && yarn --cwd ./client run build"
  },
  "license": "MIT",
  "dependencies": {
    "@passport-next/passport": "^2.1.0",
    "@passport-next/passport-google-oauth2": "^1.0.0",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "mongoose": "^5.4.14",
    "sendgrid": "^5.2.3",
    "stripe": "^6.25.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4"
  }
}
